{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "First-DFInstance"
		},
		"ADLS_IMDB_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_IMDB'"
		},
		"ADLS_first_DL_Lego_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_first_DL_Lego'"
		},
		"ADLS_IMDB_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lekanvilledatalake.dfs.core.windows.net"
		},
		"ADLS_first_DL_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lekanvilledatalake.dfs.core.windows.net"
		},
		"Http_IMDB_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datasets.imdbws.com/"
		},
		"Http_MyFirst_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "First-DFInstance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_IMDB_accountKey": {
						"value": "[parameters('ADLS_IMDB_accountKey')]"
					},
					"ADLS_first_DL_Lego_accountKey": {
						"value": "[parameters('ADLS_first_DL_Lego_accountKey')]"
					},
					"ADLS_IMDB_properties_typeProperties_url": {
						"value": "[parameters('ADLS_IMDB_properties_typeProperties_url')]"
					},
					"ADLS_first_DL_Lego_properties_typeProperties_url": {
						"value": "[parameters('ADLS_first_DL_Lego_properties_typeProperties_url')]"
					},
					"Http_IMDB_properties_typeProperties_url": {
						"value": "[parameters('Http_IMDB_properties_typeProperties_url')]"
					},
					"Http_MyFirst_properties_typeProperties_url": {
						"value": "[parameters('Http_MyFirst_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}